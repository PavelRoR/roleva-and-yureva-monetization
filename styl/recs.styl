@import '../css/bootstrap.min.css'
*
	margin 0
	padding 0
	outline none
body
	font-family: 'OpenSans';
	font-size: 16px
	background-color #fff
a
	color: #fefefe
	&:focus
		outline: 0
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight: normal
button
	border none
.video_wrapper
	margin 0 auto
	position relative
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #fff
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	margin 0 auto
	width 100%
	max-width 240px
	border-radius: 10px;
	background-color: #f2c22c
	height: 60px;
	line-height: 60px;
	font-size: 15px;
	font-family: "OpenSansBold", sans-serif;
	color: #11191b
	text-align: center;
	cursor pointer
	text-transform: uppercase
	transition: all .2s ease-in-out
	&:focus
		background-color: #f2c22c
		color #11191b
		text-decoration: none 
	&:hover
		opacity .8
		color #11191b
		text-decoration: none
		background-color: #f2c22c
.general_title
	font-size: 37px;
	font-family: "OpenSansCondensedBold", sans-serif;
	color: #11191b
	line-height: 1.384;
	text-align: center;

