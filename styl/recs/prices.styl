#prices
	width 100%
	padding 60px 0
	background-color: #f7f8f9
	background-image url(../img/last_img_1.png) ,url(../img/last_img_2.png)
	background-repeat no-repeat, no-repeat
	background-position: -290px center, 1054px center
.prices_general_title
	span
		color #b23016
.price
	background-color #fff
	border-radius: 5px
	width 100%
	max-width 320px
	margin 0 auto 25px
	border: 1px solid rgba(#b23016, .5)
	padding 20px
.price_head
	text-align: center
	padding 0 0 20px
.price_head_title
	font-family: 'OpenSansCondensedBold',sans-serif
	font-size: 20px
	span
		display block
		text-transform: uppercase
		font-size: 24px
		font-family: 'OpenSansBold',sans-serif
		color #b23016
.price_body
	padding 20px 0
	border-top: 1px solid rgba(#b23016, .5)
	border-bottom: 1px solid rgba(#b23016, .5)
	min-height 397px
.price_body_list
	padding-left: 20px
	li
		font-size: 18px
		margin-bottom: 10px
		line-height: 1.3
		color #f2c22c
		span
			color #11191b
		&:last-child
			margin-bottom: 0
.price_footer
	text-align: center
	padding 20px 0 0
.cost
	font-family: 'OpenSansCondensedBold',sans-serif
	margin-bottom: 20px
	font-size: 20px
	span
		display block
		font-family: 'OpenSansBold',sans-serif
		font-size: 24px
.cost_full
	span
		text-decoration: line-through
.cost_sale
	color red
.cost_today
	color green
	margin-bottom: 30px
.prepayment
	text-align: center
	margin 30px 0
.prepaymant_title
	font-size: 28px
	color #b23016
	font-family: 'OpenSansBold', sans-serif
.prepayment_text
	font-size: 22px
	font-family: 'OpenSansCondensedBold',sans-serif
	margin 10px 0 20px
.timer_title
	font-size: 28px
	color #b23016
	font-family: 'OpenSansBold', sans-serif
	margin 40px 0 0
	text-align: center
.alert-danger
	color: #a94442;
	background-color: #f2dede;
	padding: 10px;
	width 100%
	max-width 360px
	margin 40px auto 0
	border: 1px solid #ebccd1;
	border-radius: 4px;
	font-size: 24px;
	font-family: 'OpenSansItalic', sans-serif
	text-align: center
.flip-clock-wrapper
	width: 100%
	max-width 460px
	margin-top: 20px
.flip-clock-divider .flip-clock-label
	color #fff
.flip-clock-dot
	background-color: #b23016
.flip-clock-wrapper ul li a div div.inn
	color: #fff
	background-color: #b23016
	text-shadow: 2px 2px 2px rgba(#022a37,.6);
.cost_full .sale_cost
	text-decoration: none
.price_body_subtitle
	font-size: 20px
	font-family: 'OpenSansCondensedBold',sans-serif
	margin 10px 0 0
	span
		font-size: 18px
		font-family: 'OpenSansCondensedLight', sans-serif
		color: #aaaaaa
		border-bottom: 1px dashed
		cursor pointer